<div class='container'>
  <h1>Add a review</h1>
  <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= f.submit "Submit review", class: "btn btn-primary" %>
  <% end %>
  <p><%= link_to 'Back to restaurant', restaurant_path(@restaurant.id) %></p>
</div>
